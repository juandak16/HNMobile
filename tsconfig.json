{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "module": "NodeNext",
    "jsx": "react-native",
    "lib": ["esnext","dom"],
    "strict": true,
    "allowJs": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "types": ["node", "jest"]
  },
  "include": ["**/*.ts", "**/*.tsx", "jest.config.js"],
  "exclude": ["node_modules"]
}


